<ui version="1.00">

<!--
/* Joint
 */
-->
<vbox name="Nodes::Object::Joint::main_vb" export="1" space="4">
	<vbox align="expand">
		
		<vbox>
			<hbox align="expand">
				<combobox name="Nodes::Object::Joint::joint_cb" export="1" align="expand">
					<text>Fixed</text>
					<text>Ball</text>
					<text>Hinge</text>
					<text>Prismatic</text>
					<text>Cylindrical</text>
					<text>Suspension</text>
					<text>Wheel</text>
					<text>Particles</text>
					<text>Path</text>
				</combobox>
				<vbox space="2"/>
				<button>
					<text>Add</text>
					<callback type="clicked">Nodes::Object::Joint::add_clicked</callback>
				</button>
				<vbox space="2"/>
				<button name="Nodes::Object::Joint::remove_b" export="1">
					<text>Remove</text>
					<callback type="clicked">Nodes::Object::Joint::remove_clicked</callback>
				</button>
			</hbox>
		</vbox>
		
		<vbox space="2"/>
		
		<scrollbox align="expand" height="60">
			<listbox name="Nodes::Object::Joint::joints_lb" export="1" align="expand">
				<callback type="changed">Nodes::Object::Joint::joints_changed</callback>
			</listbox>
		</scrollbox>
		
		<vbox space="2"/>
		
		<vbox name="Nodes::Object::Joint::parameters_vb" export="1">
			<gridbox name="joint_parameters_gb" align="expand" columns="4" space="4">
				
				<label align="right"><text>Type:</text></label>
				<label name="Nodes::Object::Joint::type_l" export="1" align="left,expand"/>
				<vbox/>
				<vbox/>
				
				<vbox/>
				<checkbox name="Nodes::Object::Joint::enabled_cb" export="1" align="left,expand">
					<tooltip>The joint is enabled</tooltip>
					<text>Enabled</text>
					<callback type="clicked">Nodes::Object::Joint::enabled_clicked</callback>
				</checkbox>
				<vbox/>
				<vbox/>
				
				<vbox/>
				<checkbox name="Nodes::Object::Joint::collision_cb" export="1" align="left,expand">
					<tooltip>The joint collision flag</tooltip>
					<text>Collision</text>
					<callback type="clicked">Nodes::Object::Joint::collision_clicked</callback>
				</checkbox>
				<vbox/>
				<vbox/>
				
				<label align="right"><text>Name:</text></label>
				<editline name="Nodes::Object::Joint::name_el" export="1" align="expand">
					<tooltip>Name of the joint</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::name_pressed</callback>
				</editline>
				<vbox/>
				<vbox/>
				
				<label align="right"><text>Iterations:</text></label>
				<editline name="Nodes::Object::Joint::num_iterations_el" export="1" align="expand" validator="uint">
					<tooltip>The joint number of iterations</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::num_iterations_pressed</callback>
				</editline>
				<hslider name="Nodes::Object::Joint::num_iterations_sl" export="1" align="expand" min="1" max="16">
					<callback type="clicked">Nodes::slider_clicked</callback>
					<attach flags="max_expand">Nodes::Object::Joint::num_iterations_el</attach>
				</hslider>
				<vbox/>
				
				<label align="right"><text>Force:</text></label>
				<editline name="Nodes::Object::Joint::max_force_el" export="1" align="expand" validator="float">
					<tooltip>Maximum applied force</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::max_force_pressed</callback>
				</editline>
				<label align="right"><text>Torque:</text></label>
				<editline name="Nodes::Object::Joint::max_torque_el" export="1" align="expand" validator="float">
					<tooltip>Maximum applied torque</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::max_torque_pressed</callback>
				</editline>
				
				<label align="right"><text>LRestitution:</text></label>
				<editline name="Nodes::Object::Joint::linear_restitution_el" export="1" align="expand" validator="float">
					<tooltip>The joint linear restitution parameter</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::linear_restitution_pressed</callback>
				</editline>
				<hslider name="Nodes::Object::Joint::linear_restitution_sl" export="1" align="expand" min="0" max="1000">
					<callback type="clicked">Nodes::slider_clicked</callback>
					<attach multiplier="1000">Nodes::Object::Joint::linear_restitution_el</attach>
				</hslider>
				<vbox/>
				
				<label align="right"><text>ARestitution:</text></label>
				<editline name="Nodes::Object::Joint::angular_restitution_el" export="1" align="expand" validator="float">
					<tooltip>The joint angular restitution parameter</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::angular_restitution_pressed</callback>
				</editline>
				<hslider name="Nodes::Object::Joint::angular_restitution_sl" export="1" align="expand" min="0" max="1000">
					<callback type="clicked">Nodes::slider_clicked</callback>
					<attach multiplier="1000">Nodes::Object::Joint::angular_restitution_el</attach>
				</hslider>
				<vbox/>
				
				<label align="right"><text>LSoftness:</text></label>
				<editline name="Nodes::Object::Joint::linear_softness_el" export="1" align="expand" validator="float">
					<tooltip>The joint linear softness parameter</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::linear_softness_pressed</callback>
				</editline>
				<hslider name="Nodes::Object::Joint::linear_softness_sl" export="1" align="expand" min="0" max="1000">
					<callback type="clicked">Nodes::slider_clicked</callback>
					<attach multiplier="1000">Nodes::Object::Joint::linear_softness_el</attach>
				</hslider>
				<vbox/>
				
				<label align="right"><text>ASoftness:</text></label>
				<editline name="Nodes::Object::Joint::angular_softness_el" export="1" align="expand" validator="float">
					<tooltip>The joint angular softness parameter</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::angular_softness_pressed</callback>
				</editline>
				<hslider name="Nodes::Object::Joint::angular_softness_sl" export="1" align="expand" min="0" max="1000">
					<callback type="clicked">Nodes::slider_clicked</callback>
					<attach multiplier="1000">Nodes::Object::Joint::angular_softness_el</attach>
				</hslider>
				<vbox/>
				
				<label align="right"><text>Anchor 0:</text></label>
				<editline name="Nodes::Object::Joint::anchor_0_el[0]" export="1" align="expand" validator="float">
					<tooltip>X anchor coordinate of selected body</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::anchor_0_pressed</callback>
				</editline>
				<editline name="Nodes::Object::Joint::anchor_0_el[1]" export="1" align="expand" validator="float">
					<tooltip>Y anchor coordinate of selected body</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::anchor_0_pressed</callback>
				</editline>
				<editline name="Nodes::Object::Joint::anchor_0_el[2]" export="1" align="expand" validator="float">
					<tooltip>Z anchor coordinate of selected body</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::anchor_0_pressed</callback>
				</editline>
				
				<label align="right"><text>Anchor 1:</text></label>
				<editline name="Nodes::Object::Joint::anchor_1_el[0]" export="1" align="expand" validator="float">
					<tooltip>X anchor coordinate of second body</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::anchor_1_pressed</callback>
				</editline>
				<editline name="Nodes::Object::Joint::anchor_1_el[1]" export="1" align="expand" validator="float">
					<tooltip>Y anchor coordinate of second body</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::anchor_1_pressed</callback>
				</editline>
				<editline name="Nodes::Object::Joint::anchor_1_el[2]" export="1" align="expand" validator="float">
					<tooltip>Z anchor coordinate of second body</tooltip>
					<callback type="pressed,focus_out">Nodes::Object::Joint::anchor_1_pressed</callback>
				</editline>
				
				<button name="Nodes::Object::Joint::bind_b" export="1" align="expand" toggleable="1">
					<tooltip>Bind anchors</tooltip>
					<text>Bind</text>
				</button>
				<button align="expand">
					<tooltip>Fix anchor of selected body</tooltip>
					<text>Fix 0</text>
					<callback type="clicked">Nodes::Object::Joint::fix_0_clicked</callback>
				</button>
				<button align="expand">
					<tooltip>Fix anchor of second body</tooltip>
					<text>Fix 1</text>
					<callback type="clicked">Nodes::Object::Joint::fix_1_clicked</callback>
				</button>
				<button align="expand">
					<tooltip>Fix both anchors</tooltip>
					<text>Fix 01</text>
					<callback type="clicked">Nodes::Object::Joint::fix_01_clicked</callback>
				</button>
				
			</gridbox>
			
			<vbox space="2"/>
			<vbox name="Nodes::Object::Joint::dummy_vb" export="1"/>
			
		</vbox>
		
	</vbox>
</vbox>

